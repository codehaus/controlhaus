package org.controlhaus.webservice;

import org.apache.beehive.controls.api.bean.ControlExtension;
import org.controlhaus.webservice.ServiceControl;
import org.controlhaus.webservice.PluggableServiceControl;
import org.controlhaus.webservice.ServiceControl.Location;
import org.controlhaus.webservice.ServiceControl.WSDL;
import org.controlhaus.webservice.PluggableServiceControl.WSDLProcessorImpl;
import org.controlhaus.webservice.PluggableServiceControl.InvocationTargetImpl;
import org.controlhaus.webservice.invocation.StarwarsInvocationTarget;

/*******************************************************************************
 * 
 *
 * @author Jonathan Colwell
 */
@ControlExtension
@Location(urls = {"http://superflaco.com"})
@WSDL(path = "schemas/starwars.wsdl",
      service = "starwarsService")
@WSDLProcessorImpl
@InvocationTargetImpl(invoker = StarwarsInvocationTarget.class)
public interface Test extends PluggableServiceControl {

    public net.superflaco.Character viewChar() throws Exception;

    public String viewCharName() throws Exception;

    public net.superflaco.System viewCharSystem() throws Exception;

    public net.superflaco.Character stashChar(net.superflaco.Character ch)
        throws Exception;

    public net.superflaco.Character changeName(String name) throws Exception;
}
