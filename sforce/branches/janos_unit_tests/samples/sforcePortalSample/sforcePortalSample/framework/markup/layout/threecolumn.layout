<?xml version="1.0" encoding="UTF-8"?>
<netuix:markupDefinition xmlns:netuix="http://www.bea.com/servers/netuix/xsd/controls/netuix/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.bea.com/servers/netuix/xsd/controls/netuix/1.0.0 markup-netuix-1_0_0.xsd">
    <netuix:locale language="en"/>
    <netuix:markup>
        <netuix:gridLayout
                           title="Three Column Layout" description="This layout has three columns (left, center, right) and the portlets flow vertically in the columns."
                           columns="3" htmlLayoutUri="/framework/markup/layout/threecolumn.html.txt" iconUri="/framework/markup/layout/threecolumn.gif"
                           markupType="Layout" markupName="threeColumnLayout">
            <netuix:placeholder title="left" description="The left most placeholder in this layout."
                                flow="vertical" usingFlow="true" width="30%"
                                markupType="Placeholder" markupName="threeColumn_left">
            </netuix:placeholder>
            <netuix:placeholder title="center" description="The middle placeholder in this layout."
                                flow="vertical" usingFlow="true" width="40%"
                                markupType="Placeholder" markupName="threeColumn_center">
            </netuix:placeholder>
            <netuix:placeholder title="right" description="The right most placeholder in this layout."
                                flow="vertical" usingFlow="true" width="30%"
                                markupType="Placeholder" markupName="threeColumn_right">
            </netuix:placeholder>
        </netuix:gridLayout>
    </netuix:markup>
</netuix:markupDefinition>
