<?xml version="1.0" encoding="UTF-8"?>
<netuix:markupDefinition xmlns:netuix="http://www.bea.com/servers/netuix/xsd/controls/netuix/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.bea.com/servers/netuix/xsd/controls/netuix/1.0.0 markup-netuix-1_0_0.xsd">
    <netuix:locale language="en"/>
    <netuix:markup>
        <netuix:gridLayout
                           title="Four Column Layout" description="This layout has four columns (left, left center, right center, right) and the portlets flow vertically in the columns."
                           columns="4" htmlLayoutUri="/framework/markup/layout/fourcolumn.html.txt" iconUri="/framework/markup/layout/fourcolumn.gif"
                           markupType="Layout" markupName="fourColumnLayout">
            <netuix:placeholder title="left" description="The left most placeholder in this layout."
                                flow="vertical" usingFlow="true" width="25%"
                                markupType="Placeholder" markupName="fourColumn_left">
            </netuix:placeholder>
            <netuix:placeholder title="left center" description="The left of center placeholder in this layout."
                                flow="vertical" usingFlow="true" width="25%"
                                markupType="Placeholder" markupName="fourColumn_leftCenter">
            </netuix:placeholder>
            <netuix:placeholder title="right center" description="The right of center placeholder in this layout."
                                flow="vertical" usingFlow="true" width="25%"
                                markupType="Placeholder" markupName="fourColumn_rightCenter">
            </netuix:placeholder>
            <netuix:placeholder title="right" description="The right most placeholder in this layout."
                                flow="vertical" usingFlow="true" width="25%"
                                markupType="Placeholder" markupName="fourColumn_right">
            </netuix:placeholder>
        </netuix:gridLayout>
    </netuix:markup>
</netuix:markupDefinition>
