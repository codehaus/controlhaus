<?xml version="1.0" encoding="UTF-8"?>
<propertyset is-complete="true"
    xmlns="http://www.bea.com/servers/p13n/xsd/property-set/1.0.1"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.bea.com/servers/p13n/xsd/property-set/1.0.1 property-set-1_0_1.xsd">
    <name>AddToCartEvent</name>
    <description>An event signifying a user added a product item to
        their shopping cart.                  </description>
    <type>EVENT</type>
    <property>
        <name>user-id</name>
        <description>The user id</description>
        <is-multivalued>false</is-multivalued>
        <is-restricted>false</is-restricted>
        <datatype>Text</datatype>
    </property>
    <property>
        <name>currency</name>
        <description>The currency of the list price</description>
        <is-multivalued>false</is-multivalued>
        <is-restricted>false</is-restricted>
        <datatype>Text</datatype>
    </property>
    <property>
        <name>application-name</name>
        <description>The application or store front name</description>
        <is-multivalued>false</is-multivalued>
        <is-restricted>false</is-restricted>
        <datatype>Text</datatype>
    </property>
    <property>
        <name>session-id</name>
        <description>The session id</description>
        <is-multivalued>false</is-multivalued>
        <is-restricted>false</is-restricted>
        <datatype>Text</datatype>
    </property>
    <property>
        <name>quantity</name>
        <description>The quantity of the product added</description>
        <is-multivalued>false</is-multivalued>
        <is-restricted>false</is-restricted>
        <datatype>Float</datatype>
    </property>
    <property>
        <name>unit-list-price</name>
        <description>The list price of the product added</description>
        <is-multivalued>false</is-multivalued>
        <is-restricted>false</is-restricted>
        <datatype>Float</datatype>
    </property>
    <property>
        <name>sku</name>
        <description>The SKU of the product added</description>
        <is-multivalued>false</is-multivalued>
        <is-restricted>false</is-restricted>
        <datatype>Text</datatype>
    </property>
</propertyset>
