<?xml version="1.0" encoding="UTF-8"?>
<propertyset is-complete="true"
    xmlns="http://www.bea.com/servers/p13n/xsd/property-set/1.0.1"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.bea.com/servers/p13n/xsd/property-set/1.0.1 property-set-1_0_1.xsd">
    <name>ClickProductEvent</name>
    <description>An event signifying that a user clicked on content
        related to a product item.</description>
    <type>EVENT</type>
    <property>
        <name>user-id</name>
        <description>The user id</description>
        <is-multivalued>false</is-multivalued>
        <is-restricted>false</is-restricted>
        <datatype>Text</datatype>
    </property>
    <property>
        <name>document-id</name>
        <description>The id of content clicked</description>
        <is-multivalued>false</is-multivalued>
        <is-restricted>false</is-restricted>
        <datatype>Text</datatype>
    </property>
    <property>
        <name>application-name</name>
        <description>The application or store front name</description>
        <is-multivalued>false</is-multivalued>
        <is-restricted>false</is-restricted>
        <datatype>Text</datatype>
    </property>
    <property>
        <name>document-type</name>
        <description>The type of content clicked</description>
        <is-multivalued>false</is-multivalued>
        <is-restricted>true</is-restricted>
        <datatype>Text</datatype>
        <restricted-value>bannerAdPromotion</restricted-value>
        <restricted-value>categoryBrowse</restricted-value>
        <restricted-value>itemBrowse</restricted-value>
        <restricted-value>categoryView</restricted-value>
    </property>
    <property>
        <name>session-id</name>
        <description>The session id</description>
        <is-multivalued>false</is-multivalued>
        <is-restricted>false</is-restricted>
        <datatype>Text</datatype>
    </property>
    <property>
        <name>category-id</name>
        <description>The id of the category clicked</description>
        <is-multivalued>false</is-multivalued>
        <is-restricted>false</is-restricted>
        <datatype>Text</datatype>
    </property>
    <property>
        <name>sku</name>
        <description>The SKU of the product clicked</description>
        <is-multivalued>false</is-multivalued>
        <is-restricted>false</is-restricted>
        <datatype>Text</datatype>
    </property>
</propertyset>
